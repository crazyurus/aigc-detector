{
  "name": "aigc-detector",
  "version": "1.0.0",
  "description": "Detect if content is generated by AI",
  "keywords": [
    "aigc",
    "oclif",
    "detector"
  ],
  "homepage": "https://github.com/crazyurus/aigc-detector",
  "bugs": "https://github.com/crazyurus/aigc-detector/issues",
  "repository": "crazyurus/aigc-detector",
  "license": "GPL-3.0",
  "author": "Crazy Urus",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aigc-detector": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "pnpm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "pnpm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "pnpm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "aigc-detector",
    "commands": "./dist/commands",
    "dirname": "aigc-detector",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3.26.4",
    "@oclif/plugin-help": "^6.0.21",
    "@oclif/plugin-plugins": "^5.0.13"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.2.11",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.11.9",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.1.3",
    "eslint-config-oclif-typescript": "^3.1.6",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.4.0",
    "oclif": "^4.8.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}